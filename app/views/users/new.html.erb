<%= form_with model: @user, url: users_path, method:'post' do |form| %>
 <%= form.label :username %>
 <%= form.text_field :username, name: 'user[username]' %>

 <%= form.label :email %>
 <%= form.email_field :email, name: 'user[email]' %>

 <%= form.label :password %>
 <%= form.password_field :password, name: 'user[password]' %>

 <%= form.submit 'Create User' %>
<% end %>
