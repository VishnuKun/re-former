<%= form_with model: @user, url: users_path, method: 'post' do |form| %>
  <%= form.label :username%>
  <%= form.text_field :username, name: 'user[username]' , value: 'Anon'%>

  <%= form.label :email%>
  <%= form.email_field :email, name: 'user[email]', placeholder: 'example@123.com' %>

  <%= form.label :password %>
  <%= form.password_field :password, name: 'user[password]' %>

  <%= form.submit('Create User') %>
<% end %>